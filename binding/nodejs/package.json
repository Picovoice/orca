  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^27.5.1",
    "mkdirp": "^1.0.4",
